{
  "name": "@flyweight.cloud/yaml2jsish",
  "version": "0.2.0",
  "description": "Basic conversion of YAML to JSON/TS/JS with glob support",
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "tslint src/**/*.ts tests/**/*.ts tests/**/*.*.ts",
    "lint:fix": "tslint src/**/*.ts tests/**/*.ts tests/**/*.*.ts --fix",
    "test": "jest --config jestconfig.json",
    "deploy": "npm run lint && npm run build && npm publish --access public"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.2.0",
    "glob": "^7.2.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "jest": "^27.2.5",
    "ts-jest": "^27.0.6",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4"
  }
}
